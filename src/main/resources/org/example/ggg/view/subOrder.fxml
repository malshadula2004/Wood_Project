<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="SupplierOrderPage" prefHeight="700.0" prefWidth="960.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.ggg.controller.SubOrderController">
    <children>
        <TextField fx:id="txtSearch" layoutX="30.0" layoutY="30.0" promptText="Enter Supplier ID" />
        <Button fx:id="btnSearch" layoutX="230.0" layoutY="30.0" text="Search" onAction="#searchBySupplierId" />

        <TableView fx:id="tblSupplierOrders" layoutX="30.0" layoutY="70.0" prefHeight="600.0" prefWidth="900.0">
            <columns>
                <TableColumn fx:id="colOrderId" prefWidth="200.0" text="Order ID" />
                <TableColumn fx:id="colDate" prefWidth="300.0" text="Date" />
                <TableColumn fx:id="colSupplierId" prefWidth="200.0" text="Supplier ID" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
